{
    "name": "yii-extension/user-view-tailwind",
    "type": "library",
    "description": "View for extension user with tailwind css framework.",
    "keywords": [
        "yii3",
        "extension",
        "view",
        "tailwind"
    ],
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0",
        "yii-extension/user-settings": "^1.0",
        "yiisoft/form": "dev-enhacements-factory as 1.0",
        "yiisoft/router-fastroute": "^1.0",
        "yiisoft/router": "^1.0",
        "yiisoft/translator": "^1.0",
        "yiisoft/user": "^1.0",
        "yiisoft/yii-view": "^4.0"
    },
    "require-dev": {
        "nyholm/psr7": "^1.4",
        "vimeo/psalm": "^4.8",
        "yiisoft/di": "^1.0",
        "yiisoft/yii-event": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Yii\\Extension\\User\\View\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yii\\Extension\\User\\View\\Tests\\": "tests"
        }
    },
    "provide": {
        "yii-extension/user-view-implementation": "1.0.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "config-plugin-options": {
            "source-directory": "config"
        },
        "config-plugin": {
            "params": "params.php",
            "widgets": "widgets.php"
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "github",
            "url": "https://github.com/terabytesoftw/yii-form"
        }
    ]
}
